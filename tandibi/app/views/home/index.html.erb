<h1>Home#index</h1>
<p>Find me in app/views/home/index.html.erb</p>

<% if current_user %>
<p><%= current_user.email %></p>
<%= link_to "Sign out", destroy_user_session_path, :method => :delete %>
<% end %>

<% if !current_user %>
<%= link_to "Sign Up", new_user_registration_path, :method => :post %>
<%= link_to "Login", new_user_session_path, :method => :post %>
<% end %>
