<h1> New Post </h1>
<div class='section'>
  <%= simple_form_for @CreatePost do |f| %>
  <div class='field'>
    <div class='control'>
      <%= f.input :id %>
    </div>
  </div>
  <div class='field'>
    <div class='control'>
      <%= f.input :name %>
    </div>
  </div>
  <div class='field'>
    <div class='control'>
      <%= f.input :title, hint: false %>
    </div>
  </div>
  <div class='field'>
    <div class='control'>
      <%= f.input :content %>
    </div>
  </div>
  <%= f.button :submit %>
  <%end%>
</div>